{
  "name": "@stoqey/dior",
  "private": false,
  "version": "0.0.1",
  "description": "Order Matching Engine - match hundreds of thousands orders per second",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "register/",
    "LICENSE"
  ],
  "scripts": {
    "lint": "tslint \"src/**/*.ts\" --project tsconfig.json",
    "build": "rimraf build && tsc",
    "dev": "nodemon src/index.ts",
    "utils": "mocha src/utils/*test.ts --exit",
    "match": "mocha src/utils/matching.test.ts --exit",
    "market": "mocha src/Market/*.test.ts --exit",
    "price": "mocha src/utils/matching.price.test.ts --exit",
    "clients": "mocha src/clients/*test.ts --exit",
    "prepublishOnly": "npm run build",
    "eslint": "eslint ./src --fix --ext=ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn eslint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=4.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/stoqey/dior.git"
  },
  "keywords": [
    "graphql",
    "stoqey",
    "algorithms",
    "stocks"
  ],
  "author": {
    "name": "Ceddy Muhoza",
    "email": "ceddymuhoza@gmail.com",
    "url": "https://ceddy.org"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stoqey/dior/issues"
  },
  "homepage": "https://github.com/stoqey/dior",
  "devDependencies": {
    "@types/chai": "^4.2.15",
    "@types/debug": "^4.1.5",
    "@types/dotenv": "^8.2.0",
    "@types/lodash": "^4.14.168",
    "@types/mocha": "^8.2.0",
    "@types/moment": "^2.13.0",
    "@types/node": "^10.0.3",
    "@types/nodemon": "^1.19.0",
    "@types/source-map-support": "^0.4.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "chai": "^4.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-json": "^2.1.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^5.0.2",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "mocha": "^8.3.0",
    "prettier": "^2.0.5",
    "rimraf": "^2.5.4",
    "supertest": "^6.1.3",
    "ts-node": "^9.1.1",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": ">=2.0"
  },
  "dependencies": {
    "@sentry/node": "^6.2.0",
    "@stoqey/client-graphql": "^0.1.7",
    "@stoqey/sofa": "^0.1.12",
    "@types/chalk": "^2.2.0",
    "chalk": "^4.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "influx": "^5.8.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "nano-date": "^4.1.0",
    "nanoexpress": "^2.4.9",
    "nodemon": "^2.0.7",
    "uuid": "^8.3.2"
  }
}
